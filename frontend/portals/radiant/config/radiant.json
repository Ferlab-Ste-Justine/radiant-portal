{
  "admin": {
    "app_id": "admin",
    "admin_code": "admin"
  },
  "variant_exploration": {
    "app_id": "variant_exploration",
    "aggregations": {
      "organization": {
        "items": [{ "key": "study_code", "type": "multiple" }]
      },
      "variant": {
        "items": [
          { "key": "variant_class", "type": "multiple" },
          { "key": "chromosome", "type": "multiple" },
          { "key": "consequence", "type": "multiple" },
          { "key": "zygosity", "type": "multiple" },
          { "key": "impact_score", "type": "multiple" }
        ]
      },
      "gene": {
        "items": [
          { "key": "symbol", "type": "multiple" },
          { "key": "gnomad_pli", "type": "numerical", "defaults": { "min": 0, "max": 100, "operators": ["GreaterThan"] } },
          { "key": "gnomad_loeuf", "type": "numerical", "defaults": { "min": 0, "max": 100, "operators": ["LessThan"] } }
        ]
      },
      "frequency": {
        "items": [
          { "key": "pf", "type": "numerical", "defaults": { "min": 0, "max": 100, "operators": ["LessThan"] } },
          { "key": "gnomad_v3", "type": "numerical", "defaults": { "min": 0, "max": 100, "operators": ["LessThan"] } }
        ]
      },
      "pathogenicity": {
        "items": [
          { "key": "clinvar_interpretation", "type": "multiple" },
          { "key": "vep_impact", "type": "multiple" },
          { "key": "cadd_score", "type": "numerical", "defaults": { "min": 0, "max": 100, "operators": ["GreaterThan"] } },
          { "key": "cadd_phred", "type": "numerical", "defaults": { "min": 0, "max": 100, "operators": ["GreaterThan"] } },
          { "key": "dann_score", "type": "numerical", "defaults": { "min": 0, "max": 100, "operators": ["GreaterThan"] } },
          { "key": "fathmm_pred", "type": "multiple" },
          { "key": "lrt_pred", "type": "multiple" },
          { "key": "polyphen2_hvar_pred", "type": "multiple" },
          { "key": "revel_score", "type": "numerical", "defaults": { "min": 0, "max": 100, "operators": ["GreaterThan"] } },
          { "key": "spliceai_ds", "type": "numerical", "defaults": { "min": 0, "max": 100, "operators": ["GreaterThan"] } },
          { "key": "sift_pred", "type": "multiple" }
        ]
      },
      "occurrence": {
        "items": [{ "key": "filter", "type": "multiple" }]
      }
    }
  },
  "variant_entity": {
    "app_id": "variant_entity"
  },
  "portal": {
    "name": "Radiant",
    "navigation": {
      "dashboard": true,
      "variant": true,
      "profile": true,
      "settings": true,
      "logout": true
    }
  }
}
